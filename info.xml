<?xml version="1.0"?>
<extension key="missingoptionvalues" type="module">
  <file>missingoptionvalues</file>
  <name>Entity tables list</name>
  <description>Since CiviCRM 5.42, the entity_table field in civicrm tables will be derived from CiviCRM option group.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Pradeep Nayak</author>
    <email>pradpnayak@gmail.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/pradpnayak/missingoptionvalues</url>
    <url desc="Documentation">https://github.com/pradpnayak/missingoptionvalues</url>
    <url desc="Support">https://github.com/pradpnayak/missingoptionvalues/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2021-12-11</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.40</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/MissingOptionValues</namespace>
  </civix>
</extension>
